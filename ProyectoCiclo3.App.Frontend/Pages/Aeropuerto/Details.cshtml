@page
@model ProyectoCiclo3.App.Frontend.Pages.DetailsAeropuertoModel
@{
  var aeropuerto = Model.Aeropuerto;
}
<link rel="stylesheet" href="~/css/Details.css" />
<section class="form_wrap">

  <section class="cantact_info">
    <section class="info_title">
      <span></span>

    </section>

  </section>

  <form action="" class="form_contact">
    <h2>Ver Aeropuetos</h2>
    <div class="user_info">
      <ul class="list-group list-group-flush">
        <li class="list-group-item"> <b>Id: </b> @aeropuerto.Id</li>
        <li class="list-group-item"> <b>Nombre: </b> @aeropuerto.Nombre</li>
        <li class="list-group-item"> <b>Ciudad: </b> @aeropuerto.Ciudad</li>
        <li class="list-group-item"><b>Pais: </b> @aeropuerto.Pais</li>
        <li class="list-group-item"><b>Latitud: </b> @aeropuerto.Latitud_y</li>
        <li class="list-group-item"><b>Longitud: </b> @aeropuerto.Longitud_x</li>
      </ul>
      <a href="javascript:history.back()"><button class="btn btn-primary hBack" type="button">Atras</button></a>
    </div>
  </form>

</section>